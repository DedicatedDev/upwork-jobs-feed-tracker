{
  "manifest_version": 2,
  "name": "Upwork jobs feed tracker",
  "description": "Freelancer on upwork? Meet Upwork jobs feed tracker. Whenever new job appears in your feed you will get an instant notification!",
  "version": "2.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html#popup"
  },
  "options_page": "index.html#popup",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [
    "alarms",
    "background",
    "notifications",
    "storage",
    "https://www.upwork.com/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ]
}
